{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "HTWBerlin.FKI.VRSketchingGeometry.Editor.csproj",
            "HTWBerlin.FKI.VRSketchingGeometry.Runtime.csproj"
          ],
          "cwd": ".",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**",
            "docs/**"
          ]
        }
      ],
      "dest": "obj/api",
	  "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "toc.yml",
          "articles/toc.yml",
          "*.md",
          "Assets/VRSketchingGeometryPackage/README.md",
          "Assets/VRSketchingGeometryPackage/Samples/ExampleScenes/Scenes/README.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "docs/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "docs/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "docs/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default"
    ]
  }
}
